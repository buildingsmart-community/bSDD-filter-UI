{
  "name": "react-bsdd-search",
  "version": "0.3.10",
  "description": "bSDD Search React Component",
  "type": "module",
  "main": "./dist/react-bsdd-search.umd.js",
  "module": "./dist/react-bsdd-search.js",
  "exports": {
    ".": {
      "import": "./dist/react-bsdd-search.js",
      "require": "./dist/react-bsdd-search.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BIM-Tools/react-bsdd-search.git"
  },
  "scripts": {
    "dev": "yarn dev:search && yarn dev:selection",
    "build": "yarn build:search && yarn build:selection && node postbuild.cjs",
    "preview": "yarn preview:search && yarn preview:selection",
    "test": "yarn test:search && yarn test:selection",
    "coverage": "vitest run --coverage",
    "dev:search": "vite --config vite.config.search.ts",
    "build:search": "tsc && vite build --emptyOutDir --config vite.config.search.ts",
    "preview:search": "vite preview --config vite.config.search.ts",
    "test:search": "vitest --config vite.config.search.ts",
    "dev:selection": "vite --config vite.config.selection.ts",
    "build:selection": "tsc && vite build --emptyOutDir --config vite.config.selection.ts",
    "preview:selection": "vite preview --config vite.config.selection.ts",
    "test:selection": "vitest --config vite.config.selection.ts",
    "generate-api": "swagger-typescript-api --path https://api.swaggerhub.com/apis/buildingSMART/Dictionaries/v1/swagger.json --output ./common/src/BsddApi/ --name BsddApiBase.ts --api-class-name BsddApiBase"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "bsdd",
    "ifc"
  ],
  "author": "Jan Brouwer <jan@brewsky.nl>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@popperjs/core": "^2.11.7",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-mantine": "^3.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "fs-extra": "^11.2.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swagger-typescript-api": "^13.0.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.10.0",
    "@azure/msal-react": "^2.0.12",
    "@hello-pangea/dnd": "^16.5.0",
    "@mantine/core": "^7.6.0",
    "@mantine/hooks": "^7.6.0",
    "@reduxjs/toolkit": "^2.2.1",
    "@tabler/icons-react": "^2.47.0",
    "clsx": "^2.1.0",
    "i18next": "^23.10.0",
    "lodash": "^4.17.21",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.22.1",
    "react-select": "^5.7.0",
    "uuid": "^9.0.1"
  },
  "packageManager": "yarn@4.0.1"
}
